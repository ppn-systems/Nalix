<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Nullable>enable</Nullable>
		<IsPackable>true</IsPackable>
		<OutputType>Library</OutputType>
		<IsTrimmable>false</IsTrimmable>
		<Deterministic>true</Deterministic>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>

		<!-- Versions -->
		<Version>4.8.0</Version>
		<FileVersion>4.8.0.0</FileVersion>
		<PackageVersion>4.8.0</PackageVersion>
		<InformationalVersion>4.8.0+$(SourceRevisionId)</InformationalVersion>
	</PropertyGroup>

	<Import Project="..\Nalix.Build.props" />
	<Import Project="..\Package.Build.props" />

	<ItemGroup>
		<ProjectReference Include="..\Nalix.Common\Nalix.Common.csproj" />
	</ItemGroup>

</Project>
